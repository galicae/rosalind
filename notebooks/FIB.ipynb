   "source": [
    "def fibbonacci(n, k=1):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibbonacci(n-1, k=k) + fibbonacci(n-2, k=k) * k"
   ]
  },
